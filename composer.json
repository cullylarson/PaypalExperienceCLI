{
    "name": "cullylarson/paypal-experience-cli",
    "type": "library",
    "description": "A set of command-line tools for managing 'web experience profiles' in Paypal.",
    "keywords": ["paypal", "cli", "experience", "profile"],
    "homepage": "https://github.com/cullylarson/paypal-experience-cli",
    "license": "MIT",
    "authors": [{
        "name": "Cully Larson",
        "email": "cully.larson@gmail.com"
    }],
    "require": {
        "php": ">=5.3.0",
        "ext-curl": "*",
        "ext-json": "*",
        "paypal/rest-api-sdk-php" : "1.1.*",
        "vlucas/phpdotenv": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "PayPal\\ExperienceCli\\": "src/ExperienceCli"
        }
    },
    "bin": ["bin/create-experience-profile", "bin/list-experience-profiles", "bin/update-experience-profile", "bin/remove-experience-profile"]
}
